<h2>Show message</h2>

<ul>

  <li>
    <strong>Body:</strong>
    <%= @message.body %>
  </li>

  <li>
    <strong>Username:</strong>
    <%= @message.username %>
  </li>

  <li>
    <strong>Document:</strong>
    <%= @message.document_id %>
  </li>

</ul>

<%= link "Edit", to: message_path(@conn, :edit, @message) %>
<%= link "Back", to: message_path(@conn, :index) %>
